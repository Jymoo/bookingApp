<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#659FDAED" />
            <corners android:radius="8dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp"/>
        </shape>
    </item>
    <item android:right="100dp">
        <shape>
            <solid android:color="#60BCB392" />
            <corners android:radius="10dp" />
        </shape>
    </item>
</layer-list>
